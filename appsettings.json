{
  "ConnectionStrings": {
    "DefaultConnection": "${ConnectionStrings_DefaultConnection}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "${AllowedHosts}",
  "JWT": {
    "Issuer": "${JWT_Issuer}",
    "Audience": "${JWT_Audience}",
    "SigningKey": "${JWT_SigningKey}"
  },
  "SmtpSettings": {
    "Server": "${SmtpSettings_Server}",
    "Port": "${SmtpSettings_Port}",
    "SenderName": "${SmtpSettings_SenderName}",
    "SenderEmail": "${SmtpSettings_SenderEmail}",
    "Username": "${SmtpSettings_Username}",
    "Password": "${SmtpSettings_Password}",
    "EnableSsl": "${SmtpSettings_EnableSsl}"
  },
  "GoogleKeys": {
    "ClientId": "${GoogleKeys_ClientId}",
    "ClientSecret": "${GoogleKeys_ClientSecret}"
  },
  "FacebookKeys": {
    "AppId": "${FacebookKeys_AppId}",
    "AppSecret": "${FacebookKeys_AppSecret}"
  }
}
